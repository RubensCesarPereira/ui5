<mvc:View 
height="100%" 
controllerName="sap.ui.comp.sample.valuehelpdialog.example1.ValueHelpDialog" 
xmlns:mvc="sap.ui.core.mvc"
xmlns:l="sap.ui.layout"
xmlns:f="sap.ui.layout.form" 
xmlns:m="sap.m" 
xmlns:c="sap.ui.core" 
xmlns="sap.ui.comp.variants">
	
	
	<l:Grid defaultSpan="L9 M7 S12" width="auto">
		<l:content>
			<f:SimpleForm id="SimpleFormChange354" minWidth="1024" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout"
				title="Transferência de aproval" labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" class="editableForm">
				<f:content>
					<m:Label text="Login de"  labelFor="multiInput"/>
					<m:MultiInput id="multiInput" valueHelpRequest="onValueHelpRequest"  required="true" width="80%"/>
				
					<m:Label text="Para"/>
					<m:MultiInput id="loginPara" valueHelpRequest="onValueHelpRequest1" required="true" width="80%"/>
				
					<m:Label text="Data de"/>
					<m:DatePicker dateValue="" placeholder="Insira a data" id="datade" required="true" width="80%"/>
					<m:Label text="Data até"/>
					<m:DatePicker dateValue="" placeholder="Insira a data" id="datapara" required="true" width="80%"/>
					<m:Label/>
				
					<m:Button type="Accept" press="submit" text="Submit" width="80%"/>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</mvc:View>